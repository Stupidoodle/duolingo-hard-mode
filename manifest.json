{
	"manifest_version": 3,
	"name": "Duolingo Hard Mode",
	"version": "0.1.0",
	"description": "Replaces multiple-choice questions in Duolingo with typing input.",
	"content_scripts": [
		{
			"matches": ["*://*.duolingo.com/lesson/*"],
			"js": [
				"src/content.js",
				"src/Challenge.js",
				"src/ChallengeFactory.js",
				"src/ChallengeTranslate.js"
			]
		}
	],
	"permissions": [
		"scripting"
	]
}